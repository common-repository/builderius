<!doctype html>
<html [^ builderius_wp_function('language_attributes') ^]>
<head>
<meta charset="[^ builderius_wp_function('bloginfo', ['charset']) ^]">
<meta name="viewport" content="width=device-width, initial-scale=1'">
<link rel="profile" href="http://gmpg.org/xfn/11">
[^- builderius_wp_function('wp_head') -^]
</head>
<body [^ builderius_wp_function('body_class') ^]>
[% set the_post = builderius_wp_function('the_post') %]
<div class = "builderiusContent">
[^- builderius_wp_function('apply_filters', ['builderius_content', ''])|raw -^]
</div>
[^ builderius_wp_function('wp_footer') -^]
</body>
</html>